<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IDoorService">
                    <security mode="Message">
                        <message clientCredentialType="Windows" />
                    </security>
                </binding>
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:50071/DoorService.svc" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_IDoorService" contract="DoorServiceNamespace.IDoorService"
                name="WSDualHttpBinding_IDoorService">
                <identity>
                    <userPrincipalName value="HIICHAKI-PC\hiichaki" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>